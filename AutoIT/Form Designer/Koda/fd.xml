	<templates>
		<loop>
			<template name="Loop Example">
				<line>%INCLUDES%</line>
				<line>%OPTIONS%</line>
				<line>%EXPVARINITCODE%</line>
				<line>%GUICODE%</line>
				<line>While 1</line>
				<line> $nMsg = GUIGetMsg()</line>
				<line> Switch $nMsg</line>
				<line>  Case $GUI_EVENT_CLOSE</line>
				<line>   Exit</line>
				<line>%LOOPCODEBEGIN%</line>
				<line>  Case %CONTROL_ID%</line>
				<line>%LOOPCODEEND%</line>
				<line> EndSwitch</line>
				<line>WEnd</line>
			</template>
		</loop>
		<events>
			<template name="Events Example">
				<line>%INCLUDES%</line>
				<line>%OPTIONS%</line>
				<line>%EXPVARINITCODE%</line>
				<line>%GUICODE%</line>
				<line>While 1</line>
				<line> Sleep(100)</line>
				<line>WEnd</line>
				<line>%EVENTSCODEBEGIN%</line>
				<line>Func %EVENTNAME%()</line>
				<line/>
				<line>EndFunc</line>
				<line>%EVENTSCODEEND%</line>
			</template>
		</events>
	</templates>
